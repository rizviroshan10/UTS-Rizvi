<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecondActivity">
    <TextView
        android:id="@+id/tv_formulir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="KONFIRMASI PENDAFTARAN"
        android:textSize="20sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/tv_nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_formulir"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="15dp"
        android:text="NAMA"
        android:textSize="20sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/tv_noPendaftaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Pendaftaran"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_margin="15dp"
        android:layout_below="@+id/tv_nama"/>
    <TextView
        android:id="@+id/tv_jalurPendaftaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jalur pendaftar"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_margin="15dp"
        android:layout_below="@+id/tv_noPendaftaran"/>

</RelativeLayout>